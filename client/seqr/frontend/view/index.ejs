                                data-radio="render-settings:aspect-ratio:preset"
                                data-width="1"
                                data-height="1"
                                title="Set to 1:1 aspect ratio (square)""
                            >1:1</div>
                            <div
                                class="button"
                                data-event="render-settings:aspect-ratio:preset,radio"
                                data-radio="render-settings:aspect-ratio:preset"
                                data-width="16"
                                data-height="9"
                                title="Set to 16:9 aspect ratio (widescreen)"
                            >16:9</div>
                            <div
                                class="button"
                                data-event="render-settings:aspect-ratio:preset,radio"
                                data-radio="render-settings:aspect-ratio:preset"
                                data-width="21"
                                data-height="9"
                                title="Set to 21:9 aspect ratio (ultrawide)"
                            >21:9</div>
                            <div
                                class="button"
                                data-event="render-settings:aspect-ratio:preset,radio"
                                data-radio="render-settings:aspect-ratio:preset"
                                data-width="2"
                                data-height="1"
                                title="Set to 2:1 aspect ratio""
                            >2:1</div>
                            <div
                                class="button"
                                data-event="render-settings:aspect-ratio:preset,radio"
                                data-radio="render-settings:aspect-ratio:preset"
                                data-width="3"
                                data-height="2"
                                title="Set to 1:1 aspect ratio"
                            >3:2</div>
                            <div
                                class="button"
                                data-event="render-settings:aspect-ratio:preset,radio"
                                data-radio="render-settings:aspect-ratio:preset"
                                data-width="4"
                                data-height="3"
                                title="Set to 4:3 aspect ratio"
                            >4:3</div>
                        </div>
                    </div>
                </div>

                <div class="section background">
                    <h2 class="subtitle">
                        Background
